{
    "version":2,
    "builds" :[
        {
            "src":"server.js",
            "use":"@versel/node",
            "config":{
                "includeFiles":[
                    "dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "server.js"
        }
    ]
}